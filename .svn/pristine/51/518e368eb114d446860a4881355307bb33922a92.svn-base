li, ul, dd, dt, dd, span, h1, h2, h3, h4, h5, span, div {
  margin: 0;
  padding: 0;
}

img, th, td, tr, table {
  border: none;
}

li, ul {
  list-style: none;
}

em, a {
  text-decoration: none;
}

.clear {
  zoom: 1;
}

.clear:after {
  clear: both;
  visibility: hidden;
  height: 0;
  content: "";
  display: block;
}

/*page-header start*/
.page-header-bg {
  border-bottom: 1px solid #E0E0E0;
}

.page-header-middle {
  height: 119px;
  width: 1190px;
  margin: 0 auto;
}

.page-header-menue {
  height: 24px;
  width: 300px;
  float: right;
  font-size: 12px;
}

.page-header-menue span {
  float: left;
  border-left: 1px dashed #E0E0E0;
  margin-left: 5px;
  padding-left: 5px;
}
.page-header-menue span:hover {
  color: orange;
}
.page-header-menue a {
  color: #D02E2C;
}
.page-header-menue a:hover {
  text-decoration: underline;
  color: orange;
}

.page-user {
  border: none;
}

.shop-middle {
  height: 119px;
  width: 1190px;
  margin: 0 auto;
  margin-bottom: 50px;
}

.shop-logo {
  height: 109px;
  width: 1190px;
  padding-top: 10px;
}

.shop-logo img {
  float: left;
}
.shop-logo .shop-step {
  height: 44px;
  width: 512px;
  float: right;
  background: url(../img/flow_step.png);
}

.shop-n {
  float: left;
  margin: 10px 0;
  font-size: 16px;
  font-weight: 900;
}

.shop-extra {
  height: 200px;
  width: 1188px;
  border: 1px solid #E0E0E0;
}

.shop-title, .goodInfo {
  height: 36px;
  width: 1188px;
  background: #E2E0E0;
}

.shop-title li, .goodInfo li {
  float: left;
  text-align: center;
  line-height: 36px;
  font-size: 12px;
}

.product, .good1 {
  width: 500px;
  height: 36px;
}

.shuxing, .good2 {
  width: 190px;
  height: 36px;
}

.price, .good3 {
  width: 96px;
  height: 36px;
}

.num_con, .num {
  width: 178px;
  height: 36px;
}

.allprice, .total {
  width: 95px;
  height: 36px;
}

.do, .good6 {
  width: 118px;
  height: 36px;
}

.shop-extra p {
  width: 1188px;
  height: 70px;
  text-align: center;
  display: none;
  font-size: 12px;
}

.clean-shopping {
  height: 55px;
  width: 1188px;
  float: right;
  border: 1px dotted #E0E0E0;
  padding-top: 15px;
}

.clean-shopping a {
  height: 22px;
  width: 111px;
  float: left;
  border: 1px solid #E0E0E0;
  display: block;
  text-align: center;
  color: black;
  font-size: 12px;
  line-height: 22px;
  margin-left: 20px;
}
.clean-shopping a span {
  color: orange;
  margin-right: 5px;
  border: 1px solid #E0E0E0;
}
.clean-shopping a:hover {
  color: orange;
  text-decoration: underline;
}

.product-price {
  height: 31px;
  width: 297px;
  float: right;
  margin-right: 20px;
  text-align: center;
  border-bottom: 1px dotted #E0E0E0;
  font-size: 12px;
}
.product-price strong {
  font-size: 16px;
}

.product-price .all-price span {
  color: #D02E2C;
  font-size: 16px;
}

.goodInfo {
  background: white;
}

.good1 {
  line-height: 36px;
}
.good1:hover {
  color: orange;
  text-decoration: underline;
}
.good1 img {
  width: 30px;
  height: 30px;
  vertical-align: middle;
}

.num a {
  height: 20px;
  width: 20px;
  display: inline-block;
  text-align: center;
}
.num input {
  height: 20px;
  width: 20px;
  display: inline-block;
  text-align: center;
}

.good6 {
  color: orange;
}
.good6:hover {
  text-decoration: underline;
}

.product-price span strong {
  color: #D02E2C;
}

/*# sourceMappingURL=shopping-car.css.map */
