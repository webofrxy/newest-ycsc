li,ul,dd,dt,dd,span,h1,h2,h3,h4,h5,span,div{
    margin: 0;
    padding:0;
}
img,th,td,tr,table{
    border: none;
}
em,li,ul{
    list-style: none;
}
a{
    text-decoration: none;
}
.clearfix{
    zoom:1;
}
.clearfix:after{
    visibility:hidden;
    display:block;
    content:"";
    clear:both;
}
/*page-header start*/
@mixin bg($width:1190px,$height:119px){
    height:$height;
    width:$width;
}
.page-header-bg{
    //@include bg;
    border-bottom: 1px solid #E0E0E0;
}
.page-header-middle{
    @include bg;
    margin: 0 auto;
    
}
.page-header-menue{
   @include bg(300px,24px);
    float:right;
    font-size: 12px;
}
.page-header-menue{
    span{
        float:left;
        border-left:1px dashed #E0E0E0;
        margin-left: 5px;
        padding-left: 5px;
        &:hover{
            color: orange;
        }
    }
    a{
        color:#D02E2C;
        &:hover{
            text-decoration: underline;
            color:orange;
        }
    }
}


.page-user{
    border: none;
}
//.shop-bg{
//  @include bg;
//}
.shop-middle{
    @include bg;
    margin:0 auto;
}
.shop-logo{
    @include bg(1190px,109px);
    padding-top:10px;
}
.shop-logo{
    img{
        float:left;
        
    }
    .shop-step{
        @include bg(512px,44px);
        float:right;
        background:url(../img/flow_step.png)
    }
}
.shop-n{
    float:left;
    margin:10px 0;
    font:{
        size:16px;
        weight:900;
    } 
    
}
.shop-extra{
    @include bg(1188px,200px);
    border:1px solid #E0E0E0;
}
.shop-title{
    @include bg(1188px,80px);
    background:#E2E0E0;
}
